{
    "version": 2,
    "builds": [
      {
        "src": "weather.html",
        "use": "@vercel/static"
      },
      {
        "src": "styles.css",
        "use": "@vercel/static"
      },
      {
        "src": "weather.js",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/weather.html"
      }
    ]
  }
  