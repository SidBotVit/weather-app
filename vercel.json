{
    "version": 2,
    "builds": [
      {
        "src": "public/weather.html",
        "use": "@vercel/static"
      },
      {
        "src": "public/styles.css",
        "use": "@vercel/static"
      },
      {
        "src": "public/weather.js",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/public/weather.html"
      }
    ]
  }
  